<?xml version="1.0" encoding="UTF-8" ?>
<SolverDefinition xmlns="www.iric.net/SolverDefinition/1.0">
	<CalculationCondition>
		<Tab name="discharge" caption="Stage Discharge Boundary Conditions">
			<Content>
				<Items>
					<Item name="discharge" caption="Discharge">
						<Definition conditionType="functional">
							<Parameter valueType="real" caption="Time" />
							<Value valueType="real" caption="Discharge" />
						</Definition>
					</Item>
					<Item name="stage_at_downstream" caption="Stage at Downstream">
						<Definition conditionType="functional">
							<Parameter valueType="real" caption="Time" />
							<Value valueType="real" caption="Stage" />
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="parameters" caption="Parameters">
			<Content>
				<Items>
					<Item name="calculation_type" caption="Calculation Type">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Flow Only" />
								<Enumeration value="1" caption="Bed Variation" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="start_time" caption="Start Time">
						<Definition conditionType="constant" valueType="real" option="false" default="0" min="0" />
					</Item>
					<Item name="end_time" caption="End Time">
						<Definition conditionType="constant" valueType="real" option="false" default="0" min="start_time">
							<Dependency>
								<Condition type="always" target="start_time" />
								<Action type="setMinimum" target="start_time" />
							</Dependency>
						</Definition>
					</Item>
					<Item name="computational_timestep" caption="Computational Timestep">
						<Definition conditionType="constant" valueType="real" option="false" default="0" min="0" />
					</Item>
					<Item name="output_timestep_for_file" caption="Output Timestep for File">
						<Definition conditionType="constant" valueType="real" option="false" default="0" min="0" />
					</Item>
					<Item name="output_timestep_for_screen" caption="Output Timestep for Screen">
						<Definition conditionType="constant" valueType="real" option="false" default="0" min="0" />
					</Item>
					<Item name="bed_evolution_time" caption="Bed Evolution Time">
						<Definition conditionType="constant" valueType="real" option="false" default="0" min="0" />
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="sediment_transport" caption="Sediment Transport">
			<Content>
				<Items>
					<Item name="secondary_flow_coefficient" caption="Secondary Flow Coefficient">
						<Definition conditionType="constant" valueType="real" option="false" default="7.0" min="0" />
					</Item>
					<Item name="sediment_transport_type" caption="Sediment Transport Type">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Bed load" />
								<Enumeration value="1" caption="Bed and Suspended Load" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="suspended_sediment_type" caption="Suspended Sediment Type">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Lane-Kalinske equation" />
								<Enumeration value="1" caption="Kishi-Itakura equation" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="non_erodable_height" caption="Use Non-Erodable Height">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="0" caption="Use Non-erodible Height" />
								<Enumeration value="1" caption="Don't Use Non-erodible Height" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="rat_sed_to_equilib_sed_disch_us" caption="Ratio of Sediment to Equilibrium Sediment &lt;br&gt;Discharge Upstream">
						<Definition conditionType="constant" valueType="real" option="false" default="1.0" min="1.0" />
					</Item>
					<Item name="rat_of_sed_dt_to_flow_dt" caption="Ratio of Sediment DT to Flow DT">
						<Definition conditionType="constant" valueType="real" option="false" default="1.0" min="1.0" />
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="bed_material" caption="Bed Material">
			<Content>
				<Items>
					<Item name="bed_material_type" caption="Bed Material Type">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Uniform" />
								<Enumeration value="1" caption="Non-Uniform" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="mean_grain_diameter" caption="Mean Grain Diameter">
						<Definition conditionType="constant" valueType="real" option="false" default="0.5" min="0">
							<Dependency>
								<Condition type="isEqual" target="bed_material_type" value="0" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
					<Item name="surface_grainsize" caption="Active Layer Grainsize Distribution">
						<Definition conditionType="functional">
							<Parameter valueType="real" caption="Diameter" />
							<Value valueType="real" caption="Fraction" />
							<Dependency>
								<Condition type="isEqual" target="bed_material_type" value="1" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
					<Item name="subsurface_grainsize" caption="Sub-Active Layer Grainsize Distribution">
						<Definition conditionType="functional">
							<Parameter valueType="real" caption="Diameter" />
							<Value valueType="real" caption="Fraction" />
							<Dependency>
								<Condition type="isEqual" target="bed_material_type" value="1" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
					<Item name="exchage_layer_thickness" caption="Exchange Layer Thickness">
						<Definition conditionType="constant" valueType="real" option="false" default="1.0" min="0">
							<Dependency>
								<Condition type="isEqual" target="bed_material_type" value="1" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
					<Item name="number_of_deposition_layers" caption="Number Of Deposition Layer">
						<Definition conditionType="constant" valueType="integer" option="false" default="1" min="1"
							max="30">
							<Dependency>
								<Condition type="isEqual" target="bed_material_type" value="1" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
					<Item name="initial_layer_number" caption="Initial Layer Number">
						<Definition conditionType="constant" valueType="integer" option="false" default="1" min="1">
							<Dependency>
								<Condition type="isEqual" target="bed_material_type" value="1" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="vegetation" caption="Vegetation">
			<Content>
				<Items>
					<Item name="vegetation_density" caption="Vegetation Density">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="0" caption="Required" />
								<Enumeration value="1" caption="Not-Required" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="vegetation_height" caption="Vegetation Height">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="0" caption="Required" />
								<Enumeration value="1" caption="Not-Required" />
							</Enumerations>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="obstacle" caption="Obstacle">
			<Content>
				<Items>
					<Item name="obstacle_switch" caption="Obstacle">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Disabled" />
								<Enumeration value="1" caption="Enabled" />
							</Enumerations>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="hot_start" caption="Hot Start">
			<Content>
				<Items>
					<Item name="hot_start" caption="Hot Start">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="New" />
								<Enumeration value="1" caption="Continue" />
							</Enumerations>
						</Definition>
					</Item>
					<Item name="sediment_prn" caption="Sediment File">
						<Definition conditionType="constant" valueType="filename" option="false" default="previous.cgn">
							<Dependency>
								<Condition type="isEqual" target="hot_start" value="1" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
					<Item name="flow_prn" caption="Flow File">
						<Definition conditionType="constant" valueType="filename" option="false" default="previous.cgn">
							<Dependency>
								<Condition type="isEqual" target="hot_start" value="1" />
								<Action type="disable" />
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
	</CalculationCondition>
	<GridRelatedCondition>
		<!-- RMCD changed Bed Elevation to Elevation for consistency in MD_SWMS -->
		<Item name="aelevation" caption="Elevation">
			<Definition position="node" valueType="real" default="0" />
		</Item>
		<Item name="roughness" caption="Roughness">
			<Definition position="node" valueType="real" default="0" />
		</Item>
		<Item name="fixed_bed_elevation" caption="FixedBedElevation">
			<Definition position="node" valueType="real" default="0" />
		</Item>
		<Item name="vegetation_density" caption="VegetationDensity">
			<Definition position="node" valueType="real" default="0" />
		</Item>
		<Item name="vegetation_height" caption="VegetationHeight">
			<Definition position="node" valueType="real" default="0" />
		</Item>
		<Item name="obstacle" caption="Obstacle">
			<Definition position="node" valueType="integer" default="1" option="true">
				<Enumerations>
					<Enumeration value="1" caption="Normal" />
					<Enumeration value="2" caption="Obstacle" />
				</Enumerations>
			</Definition>
		</Item>
		<Item name="grainsize_distribution" caption="GrainSizeDistribution">
			<Definition position="node" valueType="functional" option="true">
				<Parameter valueType="integer" caption="Size Class" />
				<Value valueType="real" caption="Ratio" />
			</Definition>
		</Item>
		<Item name="roughness_example">
			<Definition position="node" valueType="complex">
				<Attributes>
					<Attribute name="Depth" caption="Depth" valueType="integer" />
					<Attribute name="Parameter1" caption="Parameter1" valueType="real" />
					<Attribute name="Parameter2" caption="Parameter2" valueType="real" />
				</Attributes>
			</Definition>
		</Item>
	</GridRelatedCondition>
</SolverDefinition>
