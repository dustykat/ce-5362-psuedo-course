<?xml version="1.0" encoding="UTF-8"?>
<SolverDefinition xmlns="www.iric.net/SolverDefinition/1.0">
	<CalculationCondition>
		<Tab name="files" caption="File Names">
			<Content>
				<Items>
					<Item name="output_type" caption="Output Type">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="CGNS Only"/>
								<Enumeration value="1" caption="CGNS and RIC-Nays"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="nays_outfile" caption="Output file name for RIC NAYS Application">
						<Definition conditionType="constant" valueType="filename" option="false" default="out.dat">
							<Dependency>
								<Condition type="isEqual" target="output_type" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outfile" caption="User defined output file name">
						<Definition conditionType="constant" valueType="filename" option="false" default="out.d">
							<Dependency>
								<Condition type="isEqual" target="output_type" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="time" caption="Time">
			<Content>
				<Items>
					<Item name="tuk" caption="Output time interval (sec)">
						<Definition conditionType="constant" valueType="real" option="false" default="10"/>
					</Item>
					<Item name="dt" caption="Calculation time step (sec)">
						<Definition conditionType="constant" valueType="real" option="false" default="0.002"/>
					</Item>
					<Item name="t_out_start" caption="Start time of output (sec)">
						<Definition conditionType="constant" valueType="real" option="false" default="30"/>
					</Item>
					<Item name="ster" caption="Start time of bed deformation (sec) &lt;br&gt;[Negative is no bed deformation]">
						<Definition conditionType="constant" valueType="real" option="false" default="30"/>
					</Item>
					<Item name="discharge" caption="Discharge Time Series">
						<Definition conditionType="functional">
							<Parameter valueType="real" caption="Time"/>
							<Value valueType="real" caption="Discharge"/>
						</Definition>
					</Item>
					<Item name="stage_at_downstream" caption="Stage at Downstream Time Series">
						<Definition conditionType="functional">
							<Parameter valueType="real" caption="Time"/>
							<Value valueType="real" caption="Discharge"/>
						</Definition>
					</Item>
					<Item name="i_sec_hour" caption="Time unit of Discharge/Water surface file">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="1" caption="Second"/>
								<Enumeration value="2" caption="Hour"/>
							</Enumerations>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="boundary_condition" caption="Boundary Condition">
			<Content>
				<Items>
					<Item name="jrep" caption="Periodic boundary condition">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="No"/>
								<Enumeration value="1" caption="Yes"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="j_wl" caption="Water surface at downstream">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Constant"/>
								<Enumeration value="1" caption="Uniform flow"/>
								<Enumeration value="2" caption="Read from file"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="h_down" caption="Constant value (m)">
						<Definition conditionType="constant" valueType="real" option="false" default="0">
							<Dependency>
								<Condition type="isEqual" target="j_wl" value="0"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="j_slope" caption="Slope for uniform flow is: ">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Calculated from geometric data"/>
								<Enumeration value="1" caption="Given"/>
							</Enumerations>
							<Dependency>
								<Condition type="isEqual" target="j_wl" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="bh_slope" caption="The slope value used in the uniform flow calculation&lt;br&gt;when the downstream water surface elevation is calculated">
						<Definition conditionType="constant" valueType="real" option="false" default="0.001">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="j_wl" value="1"/>
									<Condition type="isEqual" target="j_slope" value="1"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="j_upv" caption="Upstream velocity is calculated: ">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="1" caption="by uniform flow calculation"/>
								<Enumeration value="2" caption="from the upstream depth"/>
							</Enumerations>
							<Dependency>
								<Condition type="isEqual" target="jrep" value="0"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="j_upv_slope" caption="Slope">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="0" caption="calculated from geometric data"/>
								<Enumeration value="1" caption="given"/>
							</Enumerations>
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="jrep" value="0"/>
									<Condition type="isEqual" target="j_upv" value="1"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="upv_slope" caption="The Boundary Condition Slope">
						<Definition conditionType="constant" valueType="real" option="false" default="0.001">
							<Dependency>
								<Condition type="and">
                  <Condition type="isEqual" target="jrep" value="0"/>
                  <Condition type="isEqual" target="j_upv" value="1"/>
                  <Condition type="isEqual" target="j_upv_slope" value="1"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="initial_surface" caption="Initial water surface profile">
			<Content>
				<Items>
					<Item name="i_flow" caption="Initial water surface">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="constant slope"/>
								<Enumeration value="1" caption="line"/>
								<Enumeration value="2" caption="uniform flow"/>
								<Enumeration value="3" caption="non uniform flow"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="h_slope" caption="Initial water surface slope when i_flow = 0">
						<Definition conditionType="constant" valueType="real" option="false" default="0.001">
							<Dependency>
								<Condition type="isEqual" target="i_flow" value="0"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="x_bk" caption="Distance of the brake point from the downstream end when i_flow = 1">
						<Definition conditionType="constant" valueType="real" option="false" default="5.0">
							<Dependency>
								<Condition type="isEqual" target="i_flow" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="h_slope_1" caption="Initial water surface slope of the downstream side when i_flow=1">
						<Definition conditionType="constant" valueType="real" option="false" default="0.001">
							<Dependency>
								<Condition type="isEqual" target="i_flow" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="h_slope_2" caption="Initial water surface slope of the upstream side when i_flow=1">
						<Definition conditionType="constant" valueType="real" option="false" default="0.001">
							<Dependency>
								<Condition type="isEqual" target="i_flow" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="bed_material_and_roughness" caption="Bed material and roughness">
			<Content>
				<Items>
					<Item name="diam" caption="Diameter of bed material (mm)">
						<Definition conditionType="constant" valueType="real" option="false" default="0.55"/>
					</Item>
					<Item name="j_drg" caption="Bed roughness is:">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="calculated from bed material"/>
								<Enumeration value="1" caption="given (Manning's roughness parameter)"/>
								<Enumeration value="2" caption="Read from file"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="sn_l" caption="Manning's roughness parameter of low water channel&lt;br&gt; [cell flag = 3,11,12,13]">
						<Definition conditionType="constant" valueType="real" option="false" default="0.01">
							<Dependency>
								<Condition type="isEqual" target="j_drg" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="sn_h" caption="Manning's roughness parameter of flood channel&lt;br&gt; [cell flag = 4,5,6,7]">
						<Definition conditionType="constant" valueType="real" option="false" default="0.02">
							<Dependency>
								<Condition type="isEqual" target="j_drg" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="sn_f" caption="Manning's roughness parameter of fixed bed&lt;br&gt; [cell flag = 1,2]">
						<Definition conditionType="constant" valueType="real" option="false" default="0.01">
							<Dependency>
								<Condition type="isEqual" target="j_drg" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="snfile" caption="Input file for Manning's roughness">
						<Definition conditionType="constant" valueType="filename" option="false" default="sn.txt">
							<Dependency>
								<Condition type="isEqual" target="j_drg" value="2"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="tantc" caption="Tangent value of the critical angle of repose of the bed material">
						<Definition conditionType="constant" valueType="real" option="false" default="0.3"/>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="othercond" caption="Other condition">
			<Content>
				<Items>
					<Item name="j_snu" caption="Turbulent model">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="constant eddy viscosity"/>
								<Enumeration value="1" caption="zero equation model"/>
								<Enumeration value="2" caption="K-epsilon model"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="j_cip" caption="Finite differential method of the advection terms:">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="1" caption="upwind scheme"/>
								<Enumeration value="2" caption="CIP method"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="j_qbqs" caption="Sediment transport">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="1" caption="bed load only"/>
								<Enumeration value="2" caption="bed load and suspended load"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="snst" caption="Strength of secondary flows">
						<Definition conditionType="constant" valueType="real" option="false" default="7.0"/>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="bank_erosion" caption="Bank erosion">
			<Content>
				<Items>
					<Item name="j_bank" caption="Bank erosion">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="No"/>
								<Enumeration value="1" caption="Yes"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="i_erosion_start" caption="Bank erosion starting section number from upstream end">
						<Definition conditionType="constant" valueType="integer" option="false" default="5">
							<Dependency>
								<Condition type="isEqual" target="j_bank" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="i_erosion_end" caption="Bank erosion end section number from downstream end">
						<Definition conditionType="constant" valueType="integer" option="false" default="5">
							<Dependency>
								<Condition type="isEqual" target="j_bank" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="bheight" caption="Height of bank (m)">
						<Definition conditionType="constant" valueType="real" option="false" default="0.03">
							<Dependency>
								<Condition type="isEqual" target="j_bank" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="j_smooth" caption="Bank smoothing">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="No"/>
								<Enumeration value="1" caption="Yes"/>
							</Enumerations>
							<Dependency>
								<Condition type="isEqual" target="j_bank" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="i_smooth" caption="Smoothing range in downstream direction">
						<Definition conditionType="constant" valueType="integer" option="false" default="5">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="j_bank" value="1"/>
									<Condition type="isEqual" target="j_smooth" value="1"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="density_of_vegetation_cells" caption="Density of vegetation cells">
			<Content>
				<Items>
					<Item name="i_veg" caption="Density of vegetation cells are:">
						<Definition conditionType="constant" valueType="integer" option="true" default="1">
							<Enumerations>
								<Enumeration value="1" caption="Given"/>
								<Enumeration value="2" caption="Read from file"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="veg_lamb_1" caption="Coarse transitivity trees">
						<Definition conditionType="constant" valueType="real" option="false" default="0.2">
							<Dependency>
								<Condition type="isEqual" target="i_veg" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="veg_lamb_2" caption="Middle transmissivity trees">
						<Definition conditionType="constant" valueType="real" option="false" default="0.5">
							<Dependency>
								<Condition type="isEqual" target="i_veg" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="veg_lamb_3" caption="Dense transmissivity trees">
						<Definition conditionType="constant" valueType="real" option="false" default="0.8">
							<Dependency>
								<Condition type="isEqual" target="i_veg" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="vegfile" caption="Input file for cd_veg">
						<Definition conditionType="constant" valueType="filename" option="false" default="veg.txt">
							<Dependency>
								<Condition type="isEqual" target="i_veg" value="2"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="others" caption="Others">
			<Content>
				<Items>
					<Item name="lmax" caption="Maximum iteration times of water surface calculation">
						<Definition conditionType="constant" valueType="integer" option="false" default="10"/>
					</Item>
					<Item name="alh" caption="Relax coefficient for water surface calculation">
						<Definition conditionType="constant" valueType="real" option="false" default="0.8"/>
					</Item>
					<Item name="rho" caption="Density of water (kg/m&lt;sup&gt;3&lt;/sup&gt;)">
						<Definition conditionType="constant" valueType="real" option="false" default="1000.0"/>
					</Item>
					<Item name="spec" caption="Relative weight of the bed material">
						<Definition conditionType="constant" valueType="real" option="false" default="1.65"/>
					</Item>
					<Item name="c_tree" caption="Drag coefficient of tree">
						<Definition conditionType="constant" valueType="real" option="false" default="0.7"/>
					</Item>
					<Item name="slambda" caption="Void ratio of bed material">
						<Definition conditionType="constant" valueType="real" option="false" default="0.4"/>
					</Item>
				</Items>
			</Content>
		</Tab>
		<Tab name="restart" caption="Hot Start">
			<Content>
				<Items>
					<Item name="write_flag" caption="Output temporary files">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="No"/>
								<Enumeration value="1" caption="Yes"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="n_tempfile" caption="Number of temporary files">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="1"/>
								<Enumeration value="1" caption="2"/>
								<Enumeration value="2" caption="3"/>
								<Enumeration value="3" caption="4"/>
								<Enumeration value="4" caption="5"/>
								<Enumeration value="5" caption="6"/>
								<Enumeration value="6" caption="7"/>
								<Enumeration value="7" caption="8"/>
								<Enumeration value="8" caption="9"/>
								<Enumeration value="9" caption="10"/>
							</Enumerations>
							<Dependency>
								<Condition type="isEqual" target="write_flag" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp0" caption="Output time of temp file 0(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="20">
							<Dependency>
								<Condition type="isEqual" target="write_flag" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp1" caption="Output time of temp file 1(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="40">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="0"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp2" caption="Output time of temp file 2(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="60">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="1"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp3" caption="Output time of temp file 3(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="80">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="2"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp4" caption="Output time of temp file 4(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="100">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="3"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp5" caption="Output time of temp file 5(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="120">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="4"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp6" caption="Output time of temp file 6(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="140">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="5"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp7" caption="Output time of temp file 7(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="160">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="6"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp8" caption="Output time of temp file 8(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="180">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="7"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="outtime_tmp9" caption="Output time of temp file 9(unit:second)">
						<Definition conditionType="constant" valueType="real" option="false" default="200">
							<Dependency>
								<Condition type="and">
									<Condition type="isEqual" target="write_flag" value="1"/>
									<Condition type="isGreaterThan" target="n_tempfile" value="8"/>
								</Condition>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
					<Item name="read_flag" caption="Calculate from initial or temporary file">
						<Definition conditionType="constant" valueType="integer" option="true" default="0">
							<Enumerations>
								<Enumeration value="0" caption="Initial"/>
								<Enumeration value="1" caption="Restart"/>
							</Enumerations>
						</Definition>
					</Item>
					<Item name="tmp_readfile" caption="Input Temporary File Name">
						<Definition conditionType="constant" valueType="filename" option="false" default="tmp0.d">
							<Dependency>
								<Condition type="isEqual" target="read_flag" value="1"/>
								<Action type="enable"/>
							</Dependency>
						</Definition>
					</Item>
				</Items>
			</Content>
		</Tab>
	</CalculationCondition>
	<GridRelatedCondition>
		<!-- RMCD changed Bed Elevation to Elevation for consistency in MD_SWMS -->
		<Item name="aelevation" caption="Elevation">
